            "userName": "string",
            "password": "string"
          },
          "response_body": {
            "token": "string",
            "user": {
              "userID": "string",
              "userName": "string"
            },
            "organisations": [
              {
                "organisationID": "string",
                "organisationName": "string",
                "users":
                [
                  {
                    "userID": "string",
                    "userName": "string",
                    "permission": "int32"
                  }
                ],
                "projects": [
                  {
                    "projectID": "string",
                    "projectName": "string",
                    "users": [
                      {
                        "userID": "string",
                        "userName": "string",
                        "permission": "int32"
                      }
                    ],
                    "services": [
                      {
                        "serviceID": "string",
                        "serviceName": "string",
                        "users": [
                          {
                            "userID": "string",
                            "userName": "string",
                            "permission": "int32"
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        },
        "": {
